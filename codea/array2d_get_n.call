extern long array2d_get_n(long**, long, long);
long row1[3] = {1, 2, 3};
long row2[3] = {4, 5, 6};
long *matrix[2] = {row1, row2};
RET(array2d_get_n(matrix, 0, 0) == 1);
RET(array2d_get_n(matrix, 0, 1) == 2);
RET(array2d_get_n(matrix, 0, 2) == 3);
RET(array2d_get_n(matrix, 1, 0) == 4);
RET(array2d_get_n(matrix, 1, 1) == 5);
RET(array2d_get_n(matrix, 1, 2) == 6);
