f(noread : array of int) : int
  if (
    0 = 1
    or (
      not (
        0 = 1
        or 1 = 1
        or noread[100] = 0
      )
    )
    or (
      0 = 1
      or 0 = 2
      or 1 = 2
      or not 0 = 0
    )
    or (
      0 = 1
      or 0 = 2
      or (not 0 = 0)
      or 0 = 0
      or noread[100] = 0
    )
  ) then
    return 1;
  end;
  return 0;
end;
